        "command-line-args": "^5.2.0",
        "eslint": "^7.32.0",
        "eslint-plugin-vue": "^7.17.0",
        "gulp": "^4.0.2",
        "gulp-sass": "^4.1.1",
        "gulp-uglify": "^3.0.2",
        "is-empty": "^1.2.0",
        "jquery": "^3.6.0",
        "jsrender": "^1.0.11",
        "node-sass": "^4.14.1",
        "nw": "^0.55.0",
        "pm2": "^3.5.2",
        "popper.js": "^1.16.1",
        "rxjs": "^6.6.7",
        "rxjs-compat": "^6.6.7",
